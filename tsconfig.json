{
    "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "target": "es2021",
      "module": "commonjs",
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "preserveConstEnums": true,
      "removeComments": false,
      "sourceMap": true,
      "strict": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    },
  }
  